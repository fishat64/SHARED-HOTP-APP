  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "fs": "0.0.1-security",
    "hi-base32": "^0.5.0",
    "nedb": "^1.8.0",
    "uuid": "^8.3.2"
  }
